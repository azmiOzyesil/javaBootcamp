package sayıBulma;

public class main {

	public static void main(String[] args) {
		int[] sayılar = new int[] { 1, 5, 1, 6, 5, 1, 6, 5, 1, 6, 5, 1, 5, 1, 5, 1, 6, 3, 5, 6, 5, 1, 4, 8, 0 };
		int arananSayı = 6;
		int sayaç = 0;

		for (int sayı : sayılar) {
			if (sayı == arananSayı) {
				sayaç++;
		}
		System.out.println("Toplamda " + sayaç + " tane bulundu");
	}
}
