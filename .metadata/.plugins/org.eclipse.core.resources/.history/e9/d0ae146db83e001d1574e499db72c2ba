package reCapDemo_Classes;

public class FourOperations {

	public int sum(int num1, int num2) {
		return num1 + num2;
	}

	public int extract(int num1, int num2) {
		return num1 - num2;
	}

	public double div(int num1, int num2) {
		return num1 / num2;
	}

	public int multiple(int num1, int num2) {
		return num1 * num2;
	}

}
