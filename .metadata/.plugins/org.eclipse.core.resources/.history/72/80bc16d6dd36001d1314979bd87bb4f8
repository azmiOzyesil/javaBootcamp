
public class Main {

	public static void main(String[] args) {

		int baslangicDegeri = 100;
		int asalSayac = 0;

		while (baslangicDegeri >= 2) {
			for (int a = 2; a < baslangicDegeri; a++) {
				if (baslangicDegeri % a == 0) {
					break;
				} else {
					asalSayac++;
				}
			}
			if(asalSayac == baslangicDegeri) {
				System.out.println(baslangicDegeri+ "ASALDIR");
			}
			baslangicDegeri--;
		}
	}
}
